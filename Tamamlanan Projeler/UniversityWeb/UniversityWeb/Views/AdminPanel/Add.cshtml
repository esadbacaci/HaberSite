@model UniversityWeb.Models.AddImage


<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">

<!-- jQuery library -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

<!-- Latest compiled JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

@{Layout = null;


}



<form style="width: 50%; position: absolute;left:25%;text-align:center"
      asp-action="Add" asp-controller="AdminPanel" method="post" enctype="multipart/form-data">

    <div class="form-group">
        <label for="email">Başlık</label>
 
        <input type="text" class="form-control" name="AnnouncementTitle" >
    </div>
    <div class="form-group">

        <label for="pwd">Açıklama</label>
        <input type="text" class="form-control" name="AnnouncementDesc">
        <input type="file" class="form-control" name="AnnouncementImg">
    </div>


    <button type="submit" class="btn btn-info">Ekle</button>
</form>